{
  "name": "dev-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/web-bluetooth": "^0.0.17",
    "aphrodite": "^2.4.0",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.10.3",
    "env-cmd": "^10.1.0",
    "maplibre-gl": "^1.15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-split-pane": "^2.0.3",
    "reactstrap": "^9.1.2",
    "recharts": "^2.1.13",
    "recoil": "^0.7.3",
    "recoil-nexus": "^0.3.18",
    "styled-components": "^6.0.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-auth": "openapi --input http://account-service:8080/openapi.json --output ./src/clients/auth --client axios --additional-properties=basePath='http://account-service:8080'",
    "generate-recordings": "openapi --input http://recording-upload-service:8080/openapi.json --output ./src/clients/recording --client axios --additional-properties=basePath='http://recording-upload-service:8080'",
    "generate-models": "openapi --input http://model-service:8080/openapi.json --output ./src/clients/model --client axios --additional-properties=basePath='http://model-service:8080'",
    "start:development": "env-cmd -f .env.development react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "build:production": "env-cmd -f .env.production react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "openapi-typescript-codegen": "^0.25.0",
    "typescript": "^4.9.4"
  }
}
